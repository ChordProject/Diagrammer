{
    "name": "chordproject-diagrammer",
    "version": "0.1.8",
    "main": "dist/chordproject-diagrammer.bundle.js",
    "typings": "dist/index",
    "files": [
        "dist",
        "src/style.css"
    ],
    "description": "A TypeScript library for generating SVG chord diagrams.",
    "engines": {
        "node": ">=4.2.4"
    },
    "keywords": [
        "chordpro",
        "chordproject",
        "viewer",
        "chord",
        "diagrams"
    ],
    "author": "Jaime Mora (http://chordproject.com)",
    "scripts": {
        "start": "webpack serve --config webpack.dev.js",
        "build": "webpack --config webpack.prod.js",
        "release": "npm run build && npm publish",
        "test": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default",
        "test:ci": "npx jest --ci --runInBand --silent --colors --coverage"
    },
    "devDependencies": {
        "clean-webpack-plugin": "^3.0.0",
        "html-webpack-plugin": "^5.2.0",
        "jest": "^26.6.3",
        "ts-jest": "^26.5.2",
        "ts-loader": "^8.0.17",
        "typescript": "^4.2.2",
        "webpack": "^5.24.2",
        "webpack-cli": "^4.5.0",
        "webpack-dev-server": "^3.11.2"
    },
    "dependencies": {}
}
